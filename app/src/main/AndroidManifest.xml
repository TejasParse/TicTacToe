<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe">
        <activity android:name=".GameActivityBot"
            android:parentActivityName=".PlayerEntryBot">

        </activity>
        <activity android:name=".PlayerEntryBot"
            android:parentActivityName=".HomeActivity"/>
        <activity
            android:name=".RulesActivity"
            android:label="Rules"
            android:parentActivityName=".HomeActivity"/>
        <activity
            android:name=".PlayerEntryNoBot"
            android:label="Players Entry"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameActivityNoBot"
            android:label="Game"
            android:parentActivityName=".PlayerEntryNoBot" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>